{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/src",
      "userDataDir": "${workspaceRoot}/.chrome",
      "showAsyncStacks": true,
      "breakOnLoad": true,
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*",
        "webpack:///./node_modules/au-mod-a/*": "${workspaceRoot}/submodules/auModA/*",
        "webpack:///./node_modules/au-mod-b/*": "${workspaceRoot}/submodules/auModB/*",
        "webpack:///./node_modules/*": "${workspaceRoot}/node_modules/*"
      }
    }
  ]
}
